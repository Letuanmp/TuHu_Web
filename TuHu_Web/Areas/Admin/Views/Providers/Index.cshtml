@model IEnumerable<TuHu_Web.Models.Provider>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name_Provider)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address_Provider)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone_Number_Provider)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email_Provider)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name_Provider)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address_Provider)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone_Number_Provider)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email_Provider)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Provider }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Provider }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Provider })
        </td>
    </tr>
}

</table>
